	pages = {164--178},
	file = {Epple et al. - 2017 - A general equilibrium analysis of state and privat.pdf:/home/opmc/.mozilla/firefox/px9myza4.default/zotero/storage/42UWQTKZ/Epple et al. - 2017 - A general equilibrium analysis of state and privat.pdf:application/pdf},
}

