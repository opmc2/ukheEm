@article{buchmueller_graduate_nodate,
	title = {The {Graduate} {Wage} and {Earnings} {Premia} and the {Role} of {Non}-{Cognitive} {Skills}},
	abstract = {Estimates of the graduate earnings premium typically do not allow for the eﬀect of non-cognitive skills. Such skills are not recorded in most datasets and estimates may be contaminated by selection on these, and possibly other, unobservables. Data on a young cohort of English adolescents, followed for over 10 years, allows us to control for such skills. We ﬁnd that non-cognitive skills, themselves jointly signiﬁcantly positive on both wages and earnings, reduce the graduate premia insigniﬁcantly. It seems that non-cognitive skills raise wages/earnings for non-graduates almost by as much as they do for graduates. Motivated by the increased reliance on administrative datasets in recent research that has focused on annual rather than hourly earnings, we show that the graduate earnings diﬀerential is signiﬁcantly greater, and more sensitive to conscientiousness, than the graduate wage diﬀerential. We use estimation methods that are NOT robust to selection, and rely on Oster (2016) tests to show that an implausible degree of selection would be needed to drive our estimated diﬀerentials to zero; and that a plausible upper bound is around one-quarter below the OLS returns. We also ﬁnd heterogeneous returns by broad major group and by elite university status. Finally, UK degrees are ”classiﬁed” and we ﬁnd large degree class diﬀerentials - that seem to fully capture the eﬀects of non-cognitive skills. This result suggests that it may be reasonable to rely on estimates from administrative data provided degree classes are included.},
	language = {en},
	author = {Buchmueller, Gerda and Walker, Ian},
	keywords = {higher education, noncognitive skills, british cohort, return to education, LSYPE},
	pages = {39},
	file = {Buchmueller and Walker - The Graduate Wage and Earnings Premia and the Role.pdf:/home/opmc/.mozilla/firefox/px9myza4.default/zotero/storage/23I3VJC4/Buchmueller and Walker - The Graduate Wage and Earnings Premia and the Role.pdf:application/pdf},
}

@article{cunha_formulating_2008,
	title = {Formulating, {Identifying} and {Estimating} the {Technology} of {Cognitive} and {Noncognitive} {Skill} {Formation}},
	volume = {43},
	abstract = {This paper estimates models of the evolution of cognitive and noncognitive skills and explores the role of family environments in shaping these skills at different stages of the life cycle of the child. Central to this analysis is identiﬁcation of the technology of skill formation. We estimate a dynamic factor model to solve the problem of endogeneity of inputs and multiplicity of inputs relative to instruments. We identify the scale of the factors by estimating their effects on adult outcomes. In this fashion we avoid reliance on test scores and changes in test scores that have no natural metric. Parental investments are generally more effective in raising noncognitive skills.},
