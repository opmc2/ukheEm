	author = {Chowdry, Haroon and Crawford, Claire and Dearden, Lorraine and Goodman, Alissa and Vignoles, Anna},
	year = {2013},
	keywords = {Administrative data, Participation in higher education, Socio-economic inequality},
	pages = {431--457},
	file = {Full Text PDF:/home/opmc/.mozilla/firefox/px9myza4.default/zotero/storage/G2GJ8YFA/Chowdry et al. - 2013 - Widening participation in higher education analys.pdf:application/pdf;Snapshot:/home/opmc/.mozilla/firefox/px9myza4.default/zotero/storage/WA79CBDU/j.1467-985X.2012.01043.html:text/html},
}

@article{epple_general_2017,
	title = {A general equilibrium analysis of state and private colleges and access to higher education in the {U}.{S}.},
