@article{abadie_failure_2008,
	title = {On the {Failure} of the {Bootstrap} for {Matching} {Estimators}},
	volume = {76},
	issn = {0012-9682},
	url = {http://doi.wiley.com/10.3982/ECTA6474},
	doi = {10.3982/ECTA6474},
	abstract = {Matching estimators are widely used in empirical economics for the evaluation of programs or treatments. Researchers using matching methods often apply the bootstrap to calculate the standard errors. However, no formal justiﬁcation has been provided for the use of the bootstrap in this setting. In this article, we show that the standard bootstrap is, in general, not valid for matching estimators, even in the simple case with a single continuous covariate where the estimator is root-N consistent and asymptotically normally distributed with zero asymptotic bias. Valid inferential methods in this setting are the analytic asymptotic variance estimator of Abadie and Imbens (2006a) as well as certain modiﬁcations of the standard bootstrap, like the subsampling methods in Politis and Romano (1994).},
	language = {en},
	number = {6},
