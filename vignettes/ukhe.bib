	author = {Bonhomme, Stéphane and Lamadon, Thibaut and Manresa, Elena},
	year = {2019},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.3982/ECTA15722},
	keywords = {job mobility, sorting, bipartite networks, matched employer employee data, Two-sided heterogeneity},
