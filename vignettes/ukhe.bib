}

@article{loebbing_elementary_nodate,
	title = {An {Elementary} {Theory} of {Directed} {Technical} {Change} and {Wage} {Inequality}},
	abstract = {This paper generalizes central results from the theory of (endogenously) directed technical change to settings where technology does not take a labor-augmenting form and with arbitrarily many levels of skill. Building on simple notions of complementarity, the results remain intuitive despite their generality. The developed theory allows to study the endogenous determination of labor-replacing, that is, automation technology through the lens of directed technical change theory. In an assignment model with a continuum of differentially skilled workers and capital, where capital perfectly substitutes for labor in the production of tasks, any increase in the relative supply of skilled workers stimulates investment into improving the productivity of capital, potentially leading skill premia to increase in relative skill supply. Relatedly, trade with a skill-scarce country discourages improvements in capital productivity, potentially reversing the standard Heckscher-Ohlin effects.},
	language = {en},
	author = {Loebbing, Jonas},
	pages = {50},
	file = {Loebbing - An Elementary Theory of Directed Technical Change .pdf:/home/opmc/.mozilla/firefox/px9myza4.default/zotero/storage/4HMEDS72/Loebbing - An Elementary Theory of Directed Technical Change .pdf:application/pdf},
