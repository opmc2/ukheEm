@article{su_allocation_2004,
	title = {The allocation of public funds in a hierarchical educational system},
	volume = {28},
	issn = {0165-1889},
	url = {https://www.sciencedirect.com/science/article/pii/S0165188904000326},
	doi = {10.1016/j.jedc.2004.01.003},
	abstract = {This paper studies the dynamic effects of allocating public funds between basic and advanced education. Holding the size of total public education funds fixed, I identify the effects of their composition on aggregate efficiency and (in)equality. First, since basic education is a prerequisite for attending advanced education, there exists a lower bound on funding it: allocation policies below this lower bound are strictly Pareto dominated. Whether a corresponding lower bound on funding advanced education exists or not depends on the development stage of the economy and the size of total public funds. Secondly, while an allocation policy favoring basic education generates the usual redistribution from top to bottom, one favoring advanced education may result in reverse redistribution from bottom to top. Last, through the inter-generational link, short-run allocation policies may have long-run effects. A simple rule-of-thumb is that for an economy in its early development stage, focusing on basic education for sufficiently long duration is the only way out of polarization and low aggregate efficiency, contrary to the actual policies pursued by many of the less developed economies.},
	language = {en},
	number = {12},
	urldate = {2021-09-25},
	journal = {Journal of Economic Dynamics and Control},
	author = {Su, Xuejuan},
	month = dec,
	year = {2004},
	keywords = {Education policy, Hierarchical education, Public resource allocation, Redistributive effect},
	pages = {2485--2510},
	file = {ScienceDirect Snapshot:/home/opmc/.mozilla/firefox/px9myza4.default/zotero/storage/N7TZM7YZ/S0165188904000326.html:text/html},
}

