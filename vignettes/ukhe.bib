	year = {2010},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.3982/ECTA6551},
	keywords = {anchoring test scores, Cognitive skills, dynamic factor analysis, endogeneity of inputs, noncognitive skills, parental influence},
	pages = {883--931},
	file = {Full Text PDF:/home/opmc/.mozilla/firefox/px9myza4.default/zotero/storage/J7CVAQ49/Cunha et al. - 2010 - Estimating the Technology of Cognitive and Noncogn.pdf:application/pdf;Snapshot:/home/opmc/.mozilla/firefox/px9myza4.default/zotero/storage/Y7UNY8J6/ECTA6551.html:text/html},
}

