@article{mackowiak_survey:_2018,
	title = {Survey: {Rational} {Inattention}, a {Disciplined} {Behavioral} {Model}},
	abstract = {A recent growing body of studies shows that many important phenomena in economics are, or can be, driven by the fact that humans cannot digest all available information, but they can choose which exact pieces of information to attend to. Such phenomena span macroeconomics, ﬁnance, labor economics, political economy, and beyond. People’s choices of what information to attend to, i.e., what optimal heuristic to use, are driven by current economic conditions and determine the form of mistakes that they make. Combining these behavioral insights together with optimizing approaches of classical economics yields a new generally applicable model. The implied behavior features numerous types of empirically supported departures from existing classical models, is potentially highly practical for answering policy questions, and motivates further empirical work. One distinction from most models in behavioral economics is that this model allows for studying the adaptation of agents’ behavioral biases due to changes in policy or economic conditions.},
	language = {en},
	author = {Mackowiak, Bartosz and Matejka, Filip and Wiederholt, Mirko},
	month = oct,
	year = {2018},
	pages = {42},
	file = {Mackowiak et al. - Survey Rational Inattention, a Disciplined Behavi.pdf:/home/opmc/.mozilla/firefox/px9myza4.default/zotero/storage/MASWMJY2/Mackowiak et al. - Survey Rational Inattention, a Disciplined Behavi.pdf:application/pdf}
}

@article{fuster_expectations_2019,
