	pages = {1801--1863},
	file = {Card - 1999 - The Causal Effect of Education on Earnings.pdf:/home/opmc/.mozilla/firefox/px9myza4.default/zotero/storage/BXH236IA/Card - 1999 - The Causal Effect of Education on Earnings.pdf:application/pdf;ScienceDirect Snapshot:/home/opmc/.mozilla/firefox/px9myza4.default/zotero/storage/9UJW3SU2/S1573446399030114.html:text/html},
