@article{beleld_what_2016,
	title = {What {Drives} {Enrollment} {Gaps} in {Further} {Education}?},
	abstract = {We study students’ motives for educational attainment in a sample of 885 secondary school students in the UK. We ﬁnd that the perceived consumption value of further education explains a substantial share of students’ intentions to attain further education. The perceived consumption value alone can explain 51\% of the variation in students’ plans to go to university. We further document large gender and socio-economic gaps in the perceived consumption value. Controlling for the perceived consumption value can account for a substantial share of both the gender gap as well as the socio-economic gap in students’ intentions to continue in full-time education. Moreover, our analysis reveals that students from low socio-economic backgrounds do not only perceive the consumption value of further education to be lower, but that the perceived consumption value also plays a larger role in their educational attainment decision.},
	language = {en},
	author = {Belﬁeld, Chris and Boneva, Teodora and Rauh, Christopher and Shaw, Jonathan},
